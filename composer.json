{
    "name": "vainproject/vain",
    "description": "Project to build a laravel-driven content management system that's both, heavily extensible and easy to work with.",
    "type": "project",
    "license": "Apache-2.0",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.1.*",
        "laravelcollective/html": "5.1.*",
        "pingpong/modules": "2.1.*",
        "vainproject/vain-user": "dev-master",
        "davejamesmiller/laravel-breadcrumbs": "3.*",
        "dowilcox/knp-menu-laravel": "dev-master",
        "spatie/laravel-medialibrary": "^4.9"
    },
    "require-dev": {
        "franzl/studio": "^0.11.0",
        "phpunit/phpunit": "~5.0",
        "orchestra/testbench": "^3.1.4",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Vain\\": "src/Vain/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vain\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
